{
  "name": "ai-orchestration-engine",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "ai-training-system"
  ],
  "scripts": {
    "dev": "npm run dev -w ai-training-system",
    "test": "npm run test -w ai-training-system",
    "test:unit": "npm run test:unit -w ai-training-system",
    "test:integration": "npm run test:integration -w ai-training-system",
    "build": "npm run build -w ai-training-system",
    "setup:tests": "ts-node scripts/setup-test-structure.ts",
    "clean:tests": "powershell -Command \"Remove-Item -Path 'src/__tests__','src/agents/*/tests','src/agents/*/__tests__','test' -Recurse -Force -ErrorAction SilentlyContinue\"

  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "typescript": "~5.6.2",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
